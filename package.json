    "gatsby-plugin-google-analytics": "^2.1.7",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.6",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sharp": "^2.2.12",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-autolink-headers": "2.1.3",
    "gatsby-remark-copy-linked-files": "^2.1.6",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.12",
    "gatsby-remark-prismjs": "^3.3.5",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.14",
    "gatsby-transformer-sharp": "^2.2.6",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "lodash": "^4.17.15",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2"
  },
  "homepage": "https://github.com/0njzy0/njzy-gatsby-blog",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "now-build": "gatsby build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
